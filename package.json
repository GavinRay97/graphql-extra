{
  "name": "graphql-extra",
  "description": "GraphQL AST/SDL toolkit with extra features for code generation & testing",
  "version": "0.1.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "homepage": "https://graphql-extra.netlify.com",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/graphql-extra"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf docs && rm -rf coverage",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "format": "prettier --write src/** test/**",
    "build": "tsc -p tsconfig.build.json",
    "docs": "typedoc src",
    "test": "jest",
    "cov": "jest --coverage"
  },
  "dependencies": {
    "graphql": ">= 14.5.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@vadistic/eslint-config-node": "^0.1.6",
    "@vadistic/prettier-config": "^0.1.6",
    "@vadistic/scripts": "^0.1.6",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  }
}
