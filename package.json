{
  "name": "graphql-extra",
  "description": "GraphQL AST/SDL toolkit with extra features for code generation & testing",
  "version": "0.1.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "homepage": "https://graphql-extra.netlify.com",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/graphql-extra"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf docs && rm -rf coverage",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "format": "prettier --write src/** test/**",
    "build": "tsc -p tsconfig.build.json",
    "docs": "typedoc src",
    "test": "jest",
    "cov": "jest --coverage"
  },
  "dependencies": {
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@vadistic/eslint-config-node": "^0.1.8",
    "@vadistic/prettier-config": "^0.1.8",
    "@vadistic/scripts": "^0.1.8",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  }
}
